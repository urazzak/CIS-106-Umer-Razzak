    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="This program converts gallons to liters"/>
            <declare name="Liters" type="Real" array="False" size=""/>
            <declare name="Gallons" type="Real" array="False" size=""/>
            <comment text="Prompt user for volume in gallons"/>
            <output expression="&quot;Enter the amount of gallons: &quot;" newline="True"/>
            <input variable="Gallons"/>
            <comment text="Convert gallons into liters (metric conversion)"/>
